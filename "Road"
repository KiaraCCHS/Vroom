#include <PRIZM.h>
PRIZM prizm;

void setup() {
  prizm.PrizmBegin();
  prizm,setMotorInvert(1,1);
  }
  
void loop() {
  prizm.setMotorPowers(50,50);
  delay(4000);
  prizm.setMotorPowers(30,50);
  delay(200);
  prizm.setMotorPowers(50,30);
  delay(200);
  prizm.setMotorPowers(50,50);
  delay(1500);
  prizm.PrizmEnd();
  }
